    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/toastify-js"></script>

    <script>

        const paidDataLink = 'https://wa.me/+923332269513?text=I+Want+New+Number+Data+in+Rs+500';

        function showToast(message, type = 'success') {

            const isDark = document.body.classList.contains('dark-theme');

            let backgroundColor, icon;

            if (type === 'success') {

                backgroundColor = isDark ? 'linear-gradient(to right, #28a745, #218838)' : 'linear-gradient(to right, #28a745, #218838)';

                icon = '<i class="fa-solid fa-circle-check" style="margin-right: 10px;"></i>';

            } else {

                backgroundColor = isDark ? 'linear-gradient(to right, #dc3545, #c82333)' : 'linear-gradient(to right, #dc3545, #c82333)';

                icon = '<i class="fa-solid fa-circle-xmark" style="margin-right: 10px;"></i>';

            }

            Toastify({

                text: `${icon} ${message}`,

                duration: 3000,

                newWindow: true,

                close: true,

                gravity: "bottom", 

                position: "center",

                backgroundColor: backgroundColor,

                stopOnFocus: true,

                escapeMarkup: false,

                style: {

                    borderRadius: "10px",

                    boxShadow: "0 4px 10px rgba(0,0,0,0.2)"

                }

            }).showToast();

        }

        async function searchData() {

            const queryInput = document.getElementById("query");

            const query = queryInput.value.trim().replace(/\s+/g, '');

            const resultsContainer = document.getElementById("results");

            const noDataContainer = document.getElementById("noData");

            const loadingSpinner = document.getElementById("loading-spinner");

            

            resultsContainer.innerHTML = '';

            noDataContainer.classList.add('hidden');

            if (query === '') {

                showToast('Please enter a valid number', 'error');

                return;

            }

            loadingSpinner.classList.remove('hidden');

            const url = `https://pakdatabase.site/api/search.php?username=Kami&password=123456&search_term=${encodeURIComponent(query)}`;

            try {

                const res = await fetch(url);

                const data = await res.json();

                let output = '';

                const numbers = new Set();

                let firstCnic = null;

                for (const network in data) {

                    const records = data[network];

                    if (Array.isArray(records)) {

                        records.forEach(entry => {

                            const mobile = entry.Mobile || entry.SUB_NO || 'N/A';

                            if (!mobile || numbers.has(mobile)) return;

                            numbers.add(mobile);

                            

                            const name = entry.Name || entry.NAME || 'N/A';

                            const cnic = entry.CNIC || entry.NIC || 'N/A';

                            const address = entry.Address || entry.ADDRESS || 'N/A';

                            

                            if (cnic !== 'N/A' && !firstCnic) {

                                firstCnic = cnic;

                            }

                            output += `

                                <div class="result-box" data-cnic="${cnic}">

                                    <p><span class="label">Mobile:</span> <span class="value">${mobile}</span></p>

                                    <p><span class="label">Name:</span> <span class="value highlighted-value">${name}</span></p>

                                    <p><span class="label">ID Card:</span> <span class="value highlighted-value">${cnic}</span></p>

                                    <p><span class="label">Address:</span> <span class="value highlighted-value">${address}</span></p>

                                    <div class="action-buttons">

                                        <button class="copy-btn" onclick="copyResult(this)">Copy Data</button>

                                        <button class="get-more-btn" onclick="searchMoreByCnic(this)">Get More Numbers</button>

                                    </div>

                                </div>

                            `;

                        });

                    }

                }

                loadingSpinner.classList.add('hidden');

                

                if (output) {

                    resultsContainer.innerHTML = output;

                    showToast('Search complete!');

                } else {

                    noDataContainer.classList.remove('hidden');

                    showToast('Data not found for this number.', 'error');

                }

            } catch (err) {

                console.error("API error:", err);

                loadingSpinner.classList.add('hidden');

                noDataContainer.classList.remove('hidden');

                showToast('API response failed. Please try again.', 'error');

            }

        }

        function copyResult(button) {

            const resultBox = button.closest('.result-box');

            const dataToCopy = resultBox.textContent.split('\n').filter(line => line.trim() !== '').join('\n');

            navigator.clipboard.writeText(dataToCopy)

                .then(() => {

                    showToast('Data copied to clipboard!');

                })

                .catch(err => {

                    const textarea = document.createElement('textarea');

                    textarea.value = dataToCopy;

                    document.body.appendChild(textarea);

                    textarea.select();

                    document.execCommand('copy');

                    document.body.removeChild(textarea);

                    showToast('Data copied to clipboard (fallback)');

                });

        }

        

       function searchMoreByCnic(button) {

            const cnic = button.closest('.result-box').getAttribute('data-cnic');

            if (cnic && cnic !== 'N/A') {

                document.getElementById('query').value = cnic;

                searchData();

            } else {

                showToast('CNIC not available for this entry.', 'error');

            }

        }

        const themeToggleBtn = document.querySelector('.theme-toggle');

        const themeIcon = themeToggleBtn.querySelector('i');

        function toggleTheme() {

            const isLight = document.body.classList.toggle('light-theme');

            if (isLight) {

                themeIcon.classList.remove('fa-moon');

                themeIcon.classList.add('fa-sun');

                localStorage.setItem('theme', 'light');

            } else {

                themeIcon.classList.remove('fa-sun');

                themeIcon.classList.add('fa-moon');

                localStorage.setItem('theme', 'dark');

            }

        }

        document.addEventListener('DOMContentLoaded', () => {

            const savedTheme = localStorage.getItem('theme') || 'dark';

            if (savedTheme === 'light') {

                document.body.classList.add('light-theme');

                themeIcon.classList.remove('fa-moon');

                themeIcon.classList.add('fa-sun');

            } else {

                document.body.classList.remove('light-theme');

                themeIcon.classList.remove('fa-sun');

                themeIcon.classList.add('fa-moon');

            }

        });

    </script>

